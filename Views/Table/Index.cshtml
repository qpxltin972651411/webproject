@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@model List<String>
@{
    ViewData["Title"] = "新增表單";
}
<h2 class="page-title">表單編號</h2>
<!-- Select Unit Options Area -->
<div class="block-area">
    <div class="form-group m-b-15">
        <div style="display:inline-block;text-align:center;width:100%;height:35px;cursor:pointer;" class="block-title" onclick="slide_slow()">
            <h3 style="display:inline" id="area_tooltips">
                選擇單位 (公司 / 私人)
            </h3>
            <div style="float:right;font-size:20px;" id="collase_icon">&darr;</div>
        </div>


        <div style="text-align:center;" class="m-t-15">
            <label class="radio-inline" style="cursor:pointer;" onclick="area_toggled()">
                <span class="checkableBox checkableBox-radio">
                    <input type="radio" class="validate[required]" name="group[group3]" id="private" value="private">
                </span>
                公司
            </label>
            <label class="radio-inline" style="cursor:pointer;" onclick="area_toggled()">
                <span class="checkableBox checkableBox-radio">
                    <input type="radio" class="validate[required]" name="group[group3]" id="company" value="company">
                </span>
                私人
            </label>
        </div>

    </div>
</div>
<!-- Company Area (Required Feilds) -->
<div class="block-area unit_area" id="required">
    <!--<input class="form-control" id="formnumber" type="text" value="This is disabled..." disabled>-->
    <h3 class="block-title">單位公司資料</h3>
    <form role="form" class="form-validation-1">
        <div class="row">
            <div class="col-md-3 form-group">
                <label>單位所在縣市</label>
                <select name="country" id="country" class="form-control input-lg validate[required]" onChange="country_pick(this)">
                    <option value="">選擇縣市</option>
                    @{
                        foreach (var v in Model)
                        {
                            <option value="@v">
                                @v
                            </option>
                        }
                    }
                </select>
            </div>
            <div class="col-md-3 form-group">
                <label>單位所在地區</label>
                <select name="city" id="city" class="form-control input-lg validate[required]" onChange="city_pick(this)">
                    <option value="">選擇地區</option>
                </select>
            </div>
            <div class="col-md-6 form-group">
                <label>單位所在詳細地址</label>
                <input class="form-control input-lg validate[required]" id="areas" type="text" placeholder="詳細地址" autocomplete="off">
            </div>
        </div>

        <hr class="whiter m-t-20 m-b-20" />

        <div class="row">
            <div class="col-md-3 form-group">
                <label>工程單位</label>
                <input class="form-control input-lg validate[required]" id="unit_name" type="text" placeholder="工程單位名稱" autocomplete="off">
            </div>
            <div class="col-md-3 form-group">
                <label>統編</label>
                <input data-validatetax class="form-control input-lg validate[required,custom[number]]" id="unit_tax" type="text" placeholder="8位數" autocomplete="off">
            </div>
            <div class="col-md-2 form-group">
                <label>CEL</label>
                <input class="form-control input-lg validate[required] mask-phone_with_ddd" id="unit_cel" type="text" placeholder="行動電話" autocomplete="off">
            </div>
            <div class="col-md-2 form-group">
                <label>TEL</label>
                <input class="form-control input-lg validate[required,custom[number]]" id="unit_tel" type="text" placeholder="市內電話" autocomplete="off">
            </div>
            <div class="col-md-2 form-group">
                <label>FAX</label>
                <input class="form-control input-lg validate[required,custom[number]]" id="unit_fax" type="text" placeholder="傳真號碼" autocomplete="off">
            </div>
            <!--<label>Textarea</label>
            <textarea class="input-sm validate[required] form-control" placeholder="..."></textarea>-->
        </div>

        <input class="btn btn-sm" type="submit" value="VALIDATE">
        <button class="btn btn-sm validation-clear">CLOSE PROMPTS</button>
    </form>
</div>
<!-- Private Area (Required Feilds) -->
<div class="block-area unit_area" id="required">
    <!--<input class="form-control" id="formnumber" type="text" value="This is disabled..." disabled>-->
    <h3 class="block-title">私人客戶資料</h3>
    <form role="form" class="form-validation-1">
        <div class="row">
            <div class="col-md-3 form-group">
                <label>私人客戶所在縣市</label>
                <select name="private_country" id="private_country" class="form-control input-lg validate[required]" onChange="country_pick(this)">
                    <option value="">選擇縣市</option>
                    @{
                        foreach (var v in Model)
                        {
                            <option value="@v">
                                @v
                            </option>
                        }
                    }
                </select>
            </div>
            <div class="col-md-3 form-group">
                <label>私人客戶所在地區</label>
                <select name="private_city" id="private_city" class="form-control input-lg validate[required]" onChange="city_pick(this)">
                    <option value="">選擇地區</option>
                </select>
            </div>
            <div class="col-md-6 form-group">
                <label>私人客戶所在詳細地址</label>
                <input class="form-control input-lg validate[required]" id="private_areas" type="text" placeholder="詳細地址" autocomplete="off">
            </div>
        </div>

        <hr class="whiter m-t-20 m-b-20" />

        <div class="row">
            <div class="col-md-3 form-group">
                <label>工程單位</label>
                <input class="form-control input-lg validate[required]" id="private_unit_name" type="text" placeholder="工程單位名稱" autocomplete="off">
            </div>
            <div class="col-md-3 form-group">
                <label>CEL</label>
                <input class="form-control input-lg validate[required] mask-phone_with_ddd" id="private_unit_cel" type="text" placeholder="行動電話" autocomplete="off">
            </div>
            <div class="col-md-3 form-group">
                <label>TEL</label>
                <input class="form-control input-lg validate[required,custom[number]]" id="private_unit_tel" type="text" placeholder="市內電話" autocomplete="off">
            </div>
            <div class="col-md-3 form-group">
                <label>FAX</label>
                <input class="form-control input-lg validate[required,custom[number]]" id="private_unit_fax" type="text" placeholder="傳真號碼" autocomplete="off">
            </div>
            <!--<label>Textarea</label>
            <textarea class="input-sm validate[required] form-control" placeholder="..."></textarea>-->
        </div>

        <input class="btn btn-sm" type="submit" value="VALIDATE">
        <button class="btn btn-sm validation-clear" onclick="pop_news('切換顯示提示字')">CLOSE PROMPTS</button>
    </form>
</div>

<hr class="whiter m-t-20 m-b-20" />

<!-- Engineer Location Area -->
<div class="block-area" id="required" style="text-align:center;">
    <h3 class="block-title">工程地點填寫</h3>
    <form role="form" class="form-validation-1">
        <div class="row">
            <div class="col-md-12 form-group">
                <label>工程地點</label>
                <input class="form-control input-lg validate[required]" id="unit_location" type="text" placeholder="工程地點" autocomplete="off">
            </div>
        </div>
    </form>
</div>

<hr class="whiter m-t-20 m-b-20" />


